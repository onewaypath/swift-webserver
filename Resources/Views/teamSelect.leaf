<body>
#get(header)
#for(teamMember in teamList) {
	  #if(teamMember.username == targetUsername) {
		  
		  <h2>#(teamMember.firstname) #(teamMember.lastname)</h2>
		  <img src="images/#(teamMember.username).jpg">
		  <h3>#(teamMember.position)</h3>
		  <p>#if(bio == nil) {#raw(teamMember.bio)}</p>
	  }
    }

<footer>   
